  <script>

function check(obj)
  {
     
      if(obj.checked == true)
         document.getElementById("{!$Component.pick}").disabled=true;
      else
         document.getElementById("{!$Component.pick}").disabled=false;
  }
  function check1(obj)
  {
     
      if(obj.checked == true)
         document.getElementById("{!$Component.pick1}").disabled=true;
      else
         document.getElementById("{!$Component.pick1}").disabled=false;
  }
</script>

