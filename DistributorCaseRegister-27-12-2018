    //Constructor method
    public DistributorCaseRegisterCntlr() {
        PageId = ApexPages.currentPage().getParameters().get('id');
        //Added by Anita --Start
        SHowpage = false;
        WarrantyTaken='-–None–-';
        showdragANDdrop = false;
        selectProdList = new List < selectOption > ();
        con = new Contact();
        string idd = ApexPages.currentPage().getParameters().get('id');
        con = [select id, Name, Company__c, User_Name__c, Email from contact where id =: idd];
        system.debug('con---' + con);
        //Added by Anita--End   
        caseTypeList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult11 =case.Type.getDescribe();
        List < Schema.PicklistEntry > cou21 = fieldResult11.getPicklistValues();
        for (Schema.PicklistEntry f: cou21) {
            caseTypeList.add(new SelectOption(f.getLabel(), f.getValue()));
        }


        ComplaintTypeList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult1 = case.Complaint_Type__c.getDescribe();
        List < Schema.PicklistEntry > cou2 = fieldResult1.getPicklistValues();
        for (Schema.PicklistEntry f: cou2) {
            ComplaintTypeList.add(new SelectOption(f.getLabel(), f.getValue()));
        }


        /* CaseReasonTypeList = new List<selectOption>();

         Schema.DescribeFieldResult fieldResult2 = case.Reason.getDescribe();
         List<Schema.PicklistEntry> cou3 = fieldResult2.getPicklistValues();
         for( Schema.PicklistEntry f : cou3)
         {
           CaseReasonTypeList.add(new SelectOption(f.getLabel(), f.getValue()));
         }   */

        PriorityList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult3 =case.Priority.getDescribe();
        List < Schema.PicklistEntry > cou4 = fieldResult3.getPicklistValues();
        for (Schema.PicklistEntry f: cou4) {
            PriorityList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        WarrantyTakenList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult31 =case.Extended_Warranty_Taken__c.getDescribe();
        List < Schema.PicklistEntry > cou41 = fieldResult31.getPicklistValues();
        for (Schema.PicklistEntry f: cou41) {
            WarrantyTakenList.add(new SelectOption(f.getLabel(), f.getValue()));
        }
        WarrantyTakenList.sort();
        gdprList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult01 = case.GDPR__c.getDescribe();
        List < Schema.PicklistEntry > cou01 = fieldResult01.getPicklistValues();
        for (Schema.PicklistEntry f: cou01) {
            gdprList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        WarrantyDurationList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult311 =case.Extended_Warranty_Duration__c.getDescribe();
        List < Schema.PicklistEntry > cou311 = fieldResult311.getPicklistValues();
        for (Schema.PicklistEntry f: cou311) {
            WarrantyDurationList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        ProductFamilyList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult3111 =case.Product_Family__c.getDescribe();
        List < Schema.PicklistEntry > cou3111 = fieldResult3111.getPicklistValues();
        for (Schema.PicklistEntry f: cou3111) {
            ProductFamilyList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        TeamViewerList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult31p =case.If_customer_has_Team_viewer__c.getDescribe();
        List < Schema.PicklistEntry > cou41p = fieldResult31p.getPicklistValues();
        for (Schema.PicklistEntry f: cou41p) {
            TeamViewerList.add(new SelectOption(f.getLabel(), f.getValue()));
        }
        //Added by Anita -- Start
        VersionList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult111 = Form_Information__c.Version__c.getDescribe();
        List < Schema.PicklistEntry > cou211 = fieldResult111.getPicklistValues();
        for (Schema.PicklistEntry f: cou211) {
            VersionList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        ToolList = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult333 = Form_Information__c.Tool__c.getDescribe();
        List < Schema.PicklistEntry > cou2pp = fieldResult333.getPicklistValues();
        for (Schema.PicklistEntry f: cou2pp) {
            ToolList.add(new SelectOption(f.getLabel(), f.getValue()));
        }

        Installationlistprotv1 = new List < selectOption > ();

        Schema.DescribeFieldResult fieldResult457 = Form_Information__c.Installation__c.getDescribe();
        List < Schema.PicklistEntry > cou1452 = fieldResult457.getPicklistValues();
        for (Schema.PicklistEntry f: cou1452) {
            Installationlistprotv1.add(new SelectOption(f.getLabel(), f.getValue()));
        }
        //Added by Anita --End

    }
    // to populate the product list based on product type  
    Public void ChangeProductlist() {
        selectProdList = getproduct();
        CaseReasonTypeList = getcasereason(); // to get the dependent picklist values of Case Reason on Product family.  //Added by Anita 
    }

    //return list of products based on product type
    Public list < SelectOption > getproduct() {
        list < SelectOption > options = new List < selectOption > ();
        list < product2 > prolist = new list < product2 > ();
        if (ProductFamily != '') {
            system.debug('product family' + ProductFamily);
            for (Product2 p2: [select id, Name, Family, Active__c from Product2 where Family =: ProductFamily and Active__c != False]) {
                options.add(new SelectOption(p2.name, p2.name));
            }
        }
        return options;
    }
    // to save the changes in database //to create case and contact 
    public Pagereference DDCaseRegister() {
    
    
    if(ComplaintType!=null && ComplaintType!='' && gdpr!=null && gdpr!='' && PurchaseDate!=null &&  PurchaseDate<=system.today() && WarrantyTaken!=null && WarrantyTaken!=''  && BillNo!=null && BillNo!='' && ModalNo!=null && ModalNo!='' && SerialNo!=null && SerialNo!='') {
    
        string conid = ApexPages.currentPage().getParameters().get('id');
        boolean checkInsert = true;
        List < Case > mycase = new List < Case > ();
        List < Case > cc = new List < Case > ();
        system.debug('ModalNo ' + ModalNo);
        system.debug('BillNo' + BillNo);
        system.debug('SerialNo ' + SerialNo);
        system.debug('PurchaseDate ' + PurchaseDate);
        for (
            case ca:
                [select Id, Model_Number__c, Serial_Number__c, Bill_Number__c, Purchase_Date__c, ContactId, Complaint_Type__c from Case]) {
            if (ComplaintType == 'Repeat' && ca.Model_Number__c == ModalNo && ca.Serial_Number__c == SerialNo && ca.Purchase_Date__c == PurchaseDate && ca.Bill_Number__c == BillNo) {
                ca.status = 'Reopen';
                system.debug('cases' + ca);
                mycase.add(ca);
                checkInsert = false;
            }
        }
        if (mycase.size() > 0) {
            system.debug('Repeated');
            system.debug('mycase=' + mycase);
            update mycase;
            PageReference pr = new PageReference('/apex/Distributermycases_MMD?id=' + conid);
            pr.setRedirect(true);
            return pr;
        }
        if (checkInsert == true) {
        
            system.debug('Inserted');
            list < Product2 > prod = [select id, name from Product2 where name =: ProductName limit 1];
            system.debug('prod' + prod);
            id pid = null;
            if (prod.size() > 0) {
                pid = prod[0].id;
                case cs = new
                case ();

                cs.Bill_Number__c = BillNo;
                system.debug('Bill_Number__c====>' + cs.Bill_Number__c);
                cs.Model_Number__c = ModalNo;
                system.debug('Model_Number__c ====>' + cs.Model_Number__c);
                cs.Serial_Number__c = SerialNo;
                cs.Purchase_Date__c = date.valueOf(PurchaseDate);
                system.debug('Purchase_Date__c ====>' + cs.Purchase_Date__c);
                cs.Origin = 'Web';
                cs.Description = descc;
                cs.status = 'NEW';
                //
                cs.ContactId = conid;
                cs.type = caseType;
                cs.Complaint_Type__c = ComplaintType;
                cs.Reason = CaseReasonType;
                cs.Extended_Warranty_Taken__c = WarrantyTaken;
                cs.GDPR__c = gdpr;
                cs.Extended_Warranty_Duration__c = WarrantyDuration;
                cs.Product_Family__c = ProductFamily;
                cs.If_customer_has_Team_viewer__c = TeamViewer;
                cs.Product__c = pid;
                insert cs;



                //Below instance is used to check the condition for creating "Case Reason Form Information" according to record type  //Added by Anita
                Form_Information__c fi = new Form_Information__c();

                if ((Firmware_versionCMNDSignageform != null && Firmware_versionCMNDSignageform != '')) {

                    CMNDProTV = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('CMND Signage').RecordTypeId;
                    fi.RecordTypeId = CMNDProTV;
                    fi.Firmware_version__c = Firmware_versionCMNDSignageform;
                    fi.Scalar_version__c = Scalar_versionCMNDSignageform;
                    fi.Version__c = VersionCMNDSignageform;
                    fi.Teamviewer_ID__c = Teamviewer_IDCMNDSignageform;
                    fi.Teamviewer_Password__c = Teamviewer_PasswordCMNDSignageform;
                    fi.Description__c = protvDescriptionCMNDSignageform;
                    fi.Case__c = cs.id;
                    insert fi;
                } else if (NDA_established == True && protvDescription != null && protvDescription != '' && Firmware_version != '' && Firmware_version != null && System_Info != '' && System_Info != null) {
                    CDBform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('CDB').RecordTypeId;
                    fi.Firmware_version__c = Firmware_versionCDBform;
                    fi.System_Info__c = System_InfoCDBform;
                    fi.RecordTypeId = CDBform;
                    fi.Description__c = protvDescriptionCDBform;
                    fi.Case__c = cs.id;
                    insert fi;
                } else if (NDA_establishedInquiryform == True && protvDescriptionInquiryform != null && protvDescriptionInquiryform != '') {
                    Inquiryform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Inquiry').RecordTypeId;
                    fi.NDA_established__c = NDA_establishedInquiryform;
                    fi.Description__c = protvDescriptionInquiryform;
                    fi.RecordTypeId = Inquiryform;
                    fi.Case__c = cs.id;
                    insert fi;
                } else if (Firmware_versionSXPform != null && Firmware_versionSXPform != '' && protvDescriptionSXPform != null && protvDescriptionSXPform != '' && System_InfoSXPform != null && System_InfoSXPform != '') {
                    SXPform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('SXP').RecordTypeId;
                    fi.RecordTypeId = SXPform;
                    fi.Firmware_version__c = Firmware_versionSXPform;
                    fi.NDA_established__c = NDA_establishedSXPform;
                    fi.Description__c = protvDescriptionSXPform;
                    fi.System_Info__c = System_InfoSXPform;
                    fi.Case__c = cs.id;
                    insert fi;
                } else if (Firmware_versionMyChoiceform != null && Firmware_versionMyChoiceform != '' && protvDescriptionMyChoiceform != null && protvDescriptionMyChoiceform != '') {
                    MyChoiceform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('My Choice').RecordTypeId;
                    fi.Firmware_version__c = Firmware_versionMyChoiceform;
                    fi.Description__c = protvDescriptionMyChoiceform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = MyChoiceform;
                } else if (Firmware_versionVSecureform != null && Firmware_versionVSecureform != '' && protvDescriptionVSecureform != null && protvDescriptionVSecureform != '' && CAM_card_versionVSecureform != null && CAM_card_versionVSecureform != '' && SystemcaseVSecureform != '' && SystemcaseVSecureform != null) {
                    VSecureform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('VSecure').RecordTypeId;
                    fi.Firmware_version__c = Firmware_versionVSecureform;
                    fi.System__c = SystemcaseVSecureform;
                    fi.CAM_card_version__c = CAM_card_versionVSecureform;
                    fi.Description__c = protvDescriptionVSecureform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = VSecureform;
                    insert fi;
                } else if (Firmware_versionIssue_Signageform != null && Firmware_versionIssue_Signageform != '' && Scalar_versionIssue_Signageform != null && Scalar_versionIssue_Signageform != '' && Additional_hardwareIssue_Signageform != null && Additional_hardwareIssue_Signageform != '' && protvDescriptionIssue_Signageform != '' && protvDescriptionIssue_Signageform != null) {
                    Issue_Signageform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Issue Signage').RecordTypeId;
                    fi.Firmware_version__c = Firmware_versionIssue_Signageform;
                    fi.Scalar_version__c = Scalar_versionIssue_Signageform;
                    fi.Additional_hardware__c = Additional_hardwareIssue_Signageform;
                    fi.Description__c = protvDescriptionIssue_Signageform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = Issue_Signageform;
                    insert fi;
                } else if (Software_versionCalibrationform != null && Software_versionCalibrationform != '' && Cabling_structureCalibrationform != null && Cabling_structureCalibrationform != '' && protvDescriptionCalibrationform != null && protvDescriptionCalibrationform != '') {
                    Calibrationform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Calibration').RecordTypeId;
                    fi.Software_version__c = Software_versionCalibrationform;
                    fi.Cabling_structure__c = Cabling_structureCalibrationform;
                    fi.Description__c = protvDescriptionCalibrationform;
                    fi.Case__c = cs.id;
                    fi.Tool__c = Tool123;
                    fi.RecordTypeId = Calibrationform;
                    insert fi;
                } else if (Contact_NameProjectSupportform != null && Contact_NameProjectSupportform != '' && Contact_PhoneProjectSupportform != null && Contact_PhoneProjectSupportform != '' && protvDescriptionProjectSupportform != null && protvDescriptionProjectSupportform != '' && Software_versionProjectSupportform != null && Software_versionProjectSupportform != '' && LocatioProjectSupportform != null && LocatioProjectSupportform != '') {
                    ProjectSupportform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Project Support').RecordTypeId;
                    fi.Contact_Name__c = Contact_NameProjectSupportform;
                    fi.Contact_Phone__c = Contact_PhoneProjectSupportform;
                    fi.Software_version__c = Software_versionProjectSupportform;
                    fi.Location__c = LocatioProjectSupportform;
                    fi.Description__c = protvDescriptionProjectSupportform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = ProjectSupportform;
                    insert fi;
                } else if (Contact_NameProjectSupportform != null && Contact_NameProjectSupportform != '' && Contact_PhoneProjectSupportform != null && Contact_PhoneProjectSupportform != '' && protvDescriptionProjectSupportform != null && protvDescriptionProjectSupportform != '' && Software_versionProjectSupportform != null && Software_versionProjectSupportform != '' && LocatioProjectSupportform != null && LocatioProjectSupportform != '') {
                    ProjectSupportform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Project Support').RecordTypeId;
                    fi.Contact_Name__c = Contact_NameProjectSupportform;
                    fi.Contact_Phone__c = Contact_PhoneProjectSupportform;
                    fi.Software_version__c = Software_versionProjectSupportform;
                    fi.Location__c = LocatioProjectSupportform;
                    fi.Description__c = protvDescriptionProjectSupportform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = ProjectSupportform;
                    insert fi;
                } else
                if (App_VersionAppSigningform != null && App_VersionAppSigningform != '' && protvDescriptionAppSigningform != null && protvDescriptionAppSigningform != '') {
                    AppSigningform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('App Signing').RecordTypeId;
                    fi.App_Version__c = App_VersionAppSigningform;
                    fi.Description__c = protvDescriptionAppSigningform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = AppSigningform;
                    insert fi;
                } else
                if (Monitor_typeCRD50form != null && Monitor_typeCRD50form != '' && Monitor_FirmwareCRD50form != null && Monitor_FirmwareCRD50form != '' && Scalar_firmwareCRD50form != null && Scalar_firmwareCRD50form != '' && protvDescriptionCRD50form != null && protvDescriptionCRD50form != '') {
                    CRD50form = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('CRD 50').RecordTypeId;
                    fi.Monitor_type__c = Monitor_typeCRD50form;
                    fi.Monitor_Firmware__c = Monitor_FirmwareCRD50form;
                    fi.Scalar_firmware__c = Scalar_firmwareCRD50form;
                    fi.Description__c = protvDescriptionCRD50form;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = CRD50form;
                    insert fi;
                } else
                if (Monitor_typeCRD50form != null && Monitor_typeCRD50form != '' && Monitor_FirmwareCRD50form != null && Monitor_FirmwareCRD50form != '' && Scalar_firmwareCRD50form != null && Scalar_firmwareCRD50form != '' && protvDescriptionCRD50form != null && protvDescriptionCRD50form != '') {
                    Accessoriesform = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('Accessories').RecordTypeId;
                    fi.Device__c = DeviceAccessoriesform;
                    fi.Description__c = protvDescriptionAccessoriesform;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = Accessoriesform;
                    insert fi;
                } else
                if (Locationprotv1 != null && Locationprotv1 != '' && FirmwareVersionprotv1 != null && FirmwareVersionprotv1 != '' && Installationprotv1 != null && Installationprotv1 != '' && Systemdescriptionprotv1 != null && Systemdescriptionprotv1 != '' && Unitsaffectedprotv1 != null && protvDescriptionprotv1 != null && protvDescriptionprotv1 != '') {
                    CMNDProTV1 = Schema.SObjectType.Form_Information__c.RecordTypeInfosByName.get('CMND Pro TV').RecordTypeId;
                    fi.Case__c = cs.id;
                    fi.RecordTypeId = CMNDProTV1;
                    fi.Location__c = Locationprotv1;
                    fi.Firmware_version__c = FirmwareVersionprotv1;
                    fi.Installation__c = Installationprotv1;
                    fi.System_description__c = Systemdescriptionprotv1;
                    fi.Units_affected__c = Unitsaffectedprotv1;
                    insert fi;
                }
                //Added by Anita -- End    

                if (cs != null) {
                    emailTocustomer();
                    PageReference pr = new pagereference('/apex/Distributermycases_MMD?id=' + conid);
                    pr.seTRedirect(true);
                    return pr;
                }
            }
          }
        }
        else{
                     ApexPages.addmessage(new ApexPages.message(ApexPages.severity.WARNING, 'Please fill required fields:'));
                     
        }
        return null;

    }

    public PageReference getAttachment() {
        return null;
    }

    //This method is used to get "case reason" dependent on "product family" //Added by Anita -- Start
    Public list < SelectOption > getcasereason() {
        case ca2 = new
        case ();
        List < selectOption > CaseReasonType = new List < selectOption > ();
        If(ProductFamily == 'High Bright Signage Display') {
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            return CaseReasonType;
        } else if (ProductFamily == 'Videowall Display') {
            List < selectOption > CaseReasonType1 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            CaseReasonType.add(new SelectOption('Calibration', 'Calibration'));
            return CaseReasonType;
        } else if (ProductFamily == 'Indoor LED Display') {
            List < selectOption > CaseReasonType2 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Project Support', 'Project Support'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            return CaseReasonType;
        } else if (ProductFamily == 'Signage Display') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            CaseReasonType.add(new SelectOption('App Signing', 'App Signing'));
            return CaseReasonType;
        } else if (ProductFamily == 'ProTv') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            CaseReasonType.add(new SelectOption('CDB', 'CDB'));
            CaseReasonType.add(new SelectOption('SXP', 'SXP'));
            CaseReasonType.add(new SelectOption('MyChoice', 'MyChoice'));
            CaseReasonType.add(new SelectOption('Vsecure', 'Vsecure'));
            return CaseReasonType;
        } else if (ProductFamily == 'LED') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Project Support', 'Project Support'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            return CaseReasonType;
        } else if (ProductFamily == 'Touch') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CMND', 'CMND'));
            CaseReasonType.add(new SelectOption('Inquiry', 'Inquiry'));
            CaseReasonType.add(new SelectOption('Issue', 'Issue'));
            CaseReasonType.add(new SelectOption('App Signing', 'App Signing'));
            return CaseReasonType;
        } else if (ProductFamily == 'OPS') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('CRD50', 'CRD50'));
            return CaseReasonType;
        } else if (ProductFamily == 'Accesories') {
            List < selectOption > CaseReasonType3 = new List < selectOption > ();
            CaseReasonType.add(new SelectOption('-None-', '-None-'));
            CaseReasonType.add(new SelectOption('Remote Control', 'Remote Control'));
            CaseReasonType.add(new SelectOption('CRD41', 'CRD41'));
            return CaseReasonType;
        }
        return null;
    }
    //The method to create "case reasonform information" dependent on "product family" end
    //This method is used to create "case reason" dependent on "case" form functionality start //Added by Anita -Start
    public Void casereasonform() {
        if (CaseReasonType == 'CMND' && ProductFamily == 'ProTv') {
            showinteger = 13;
        } else if (CaseReasonType == 'Issue' && ProductFamily == 'ProTv') {
            showinteger = 14;
        } else if (CaseReasonType == 'CMND' && ProductFamily != 'ProTv') {
            showinteger = 1;
        } else if (CaseReasonType == 'Inquiry') {
            showinteger = 2;
        } else if (CaseReasonType == 'Issue') {
            showinteger = 3;
        } else if (CaseReasonType == 'Calibration') {
            showinteger = 4;
        } else if (CaseReasonType == 'Project Support') {
            showinteger = 5;
        } else if (CaseReasonType == 'App Signing') {
            showinteger = 6;
        } else if (CaseReasonType == 'CDB') {
            showinteger = 7;
        } else if (CaseReasonType == 'SXP') {
            showinteger = 8;
        } else if (CaseReasonType == 'MyChoice') {
            showinteger = 9;
        } else if (CaseReasonType == 'Vsecure') {
            showinteger = 10;
        } else if (CaseReasonType == 'CRD50') {
            showinteger = 11;
        } else if (CaseReasonType == 'Remote Control') {
            showinteger = 12;
        } else if (CaseReasonType == 'CRD41') {
            showinteger = 12;
        }

    }

    //The method to create "case reason" dependent on "case form" functionality end  //Added by Anita -End          

    public void emailTocustomer() {

        string conid = ApexPages.currentPage().getParameters().get('id');
        contact con = new Contact();
        con = [select id, name, email, lastname, User_Name__c, Password__c from contact where id =: conid];

        if (con != null) {
            Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();
            list < string > toSent = new list < string > ();
            toSent.add(con.email);
            email.setToaddresses(toSent);
            email.setTargetObjectId(con.id);
            //string bodym='Dear '+con.name+',<br/><br />Your Case has been succesufully Submitted<br/><br/>Thanks and Regards,<br/>' ;  
            EmailTemplate ev = [select id, name, Subject, Body from EmailTemplate where name = 'Case submission confirmation'];
            email.setTemplateID(ev.Id);
            //email.setSubject(ev.Subject);  
            //email.setSubject(ev.Subject);  
            system.debug('++++++++++++++++++++++' + ev.id);
            Messaging.SendEmailResult[] r = Messaging.sendEmail(new Messaging.SingleEmailMessage[] {
                email
            });

        }
    }
    // method to show attachment block  added by indresh
    public PageReference myAttactmentmethod() {
        showdragANDdrop = true;
        SHowpage = true;
        return null;
    }
    //method to back on main page from attachment block added by indresh
    public void backmethod() {
        String currentid = ApexPages.currentpage().getParameters().get('id');
        try{
        DragDropToCloud__Cloud_Documents__c cc = new DragDropToCloud__Cloud_Documents__c();
        cc = [select id, name from DragDropToCloud__Cloud_Documents__c where DragDropToCloud__Contact__c =: currentid order by createddate desc limit 1];
        Filename = cc.name;
        }catch(exception e){
        
        }
        showdragANDdrop = false;
        SHowpage = false;
    }

}
