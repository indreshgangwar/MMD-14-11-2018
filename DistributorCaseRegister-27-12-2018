public with sharing class DistributorCaseRegisterCntlr {

    public Contact con {
        get;
        set;
    }
    public list < selectOption > caseTypeList {
        get;
        set;
    }
    public string caseType {
        get;
        set;
    }
    public list < selectOption > ComplaintTypeList {
        get;
        set;
    }
    public string ComplaintType {
        get;
        set;
    }
    public list < selectOption > CaseReasonTypeList {
        get;
        set;
    }
    public string CaseReasonType {
        get;
        set;
    }
